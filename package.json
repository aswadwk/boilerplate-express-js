{
  "name": "boilerplate-express",
  "version": "1.0.0",
  "description": "boilerplate express js",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "node src/main.js",
    "dev": "nodemon src/main.js",
    "test:watch": "jest --watchAll --coverage --setupFiles -i --testTimeout=50000",
    "test": "jest --setupFiles -i",
    "lint": "eslint --fix --ext .js ."
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "keywords": [
    "express",
    "js",
    "node",
    "js",
    "api"
  ],
  "author": "Hajar Aswad - hajaraswadkom@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.3",
    "@types/supertest": "^6.0.3",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prisma": "^5.22.0",
    "supertest": "^7.1.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0"
  }
}